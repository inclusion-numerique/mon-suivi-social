{
  "name": "@mss/cli",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "tsc": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "build": "tsc",
    "with-env": "dotenv -e ../../.env --",
    "with-remote-env": "dotenv -e ../../.env.remote -e ../../.env --",
    "env:from-cdk": "pnpm with-env ts-node src/commands/envFromCdk.ts",
    "fixtures:load": "pnpm with-env ts-node src/commands/loadFixtures.ts",
    "remote:fixtures:load": "pnpm with-remote-env ts-node src/commands/loadFixtures.ts"
  },
  "engines": {
    "node": "^18.13"
  },
  "dependencies": {
    "@mss/web": "*",
    "axios": "^1.2.6",
    "csv-parse": "^5.3.3",
    "csv-stringify": "^6.2.3",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "typescript": "^4.9.5"
  }
}
