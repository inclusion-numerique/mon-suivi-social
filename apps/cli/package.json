{
  "name": "@mss/cli",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "cli": "ts-node src/index.ts",
    "lint": "eslint src",
    "tsc": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "build": "tsc",
    "with-env": "dotenv -e ../../.env --",
    "with-remote-env": "dotenv -e ../../.env.remote -e ../../.env --",
    "env:from-cdk": "pnpm with-env ts-node src/commands/envFromCdk.ts",
    "fixtures:load": "pnpm with-env ts-node src/commands/loadFixtures.ts",
    "github:deployment:create": "pnpm with-env ts-node src/commands/createGithubDeployment.ts",
    "github:deployment:update": "pnpm with-env ts-node src/commands/updateGithubDeployment.ts",
    "remote:fixtures:load": "pnpm with-remote-env ts-node src/commands/loadFixtures.ts"
  },
  "engines": {
    "node": "^18.13"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.2",
    "@mss/web": "*",
    "axios": "^1.3.3",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "csv-parse": "^5.3.5",
    "csv-stringify": "^6.2.4",
    "lodash": "^4.17.21",
    "octokit": "^2.0.14",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "typescript": "^4.9.5"
  }
}
