{
  "name": "@mss/web",
  "version": "0.1.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "pnpm with-env next dev",
    "build": "next build",
    "scalingo-postbuild": "next build && modclean -Pr && npm prune --omit=dev",
    "modclean": "modclean -Pr",
    "modclean:root": "modclean -Pr -p ./../..",
    "start": "next start",
    "lint": "next lint && ../../packages/lint/node_modules/.bin/stylelint \"src/**/*.css\"",
    "lint:eslint": "next lint",
    "lint:style": "../../packages/lint/node_modules/.bin/stylelint \"src/**/*.css\"",
    "tsc": "tsc --noEmit",
    "postinstall": "npm run setup:dsfr",
    "setup:dsfr": "FROM=\"./node_modules/@gouvfr/dsfr/dist\"; TO=\"public/dsfr\"; if [ ! -d \"$FROM\" ]; then FROM=\"../../node_modules/@gouvfr/dsfr/dist\"; fi; mkdir -p public/dsfr/utility && cp $FROM/dsfr.module.min.js $TO/ && cp $FROM/dsfr.module.min.js.map $TO/ && cp $FROM/dsfr.nomodule.min.js $TO/ && cp $FROM/dsfr.nomodule.min.js.map $TO/ && cp $FROM/dsfr.min.css $TO/ && cp $FROM/utility/utility.min.css $TO/utility/ && cp -R $FROM/icons $TO/ && cp -R $FROM/artwork $TO/ && cp -R $FROM/fonts $TO/",
    "test": "jest",
    "with-env": "dotenv -e ../../.env --",
    "with-remote-env": "dotenv -e ../../.env.remote -e ../../.env --",
    "prisma:dev": "pnpm with-env prisma studio --port 5556",
    "prisma:db:push": "pnpm with-env prisma db push --skip-generate",
    "prisma:remote:db:push": "pnpm with-remote-env prisma db push --skip-generate",
    "prisma:migrate:dev": "pnpm with-env prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:pull": "pnpm with-env prisma db pull",
    "prisma": "pnpm with-env prisma"
  },
  "engines": {
    "node": "^18.13"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.278.0",
    "@aws-sdk/s3-request-presigner": "^3.278.0",
    "@gouvfr/dsfr": "^1.8.5",
    "@hookform/resolvers": "^2.9.11",
    "@mss/emails": "*",
    "@mss/storybook": "*",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "@sentry/browser": "^7.38.0",
    "@sentry/nextjs": "^7.38.0",
    "@tanstack/react-query": "^4.24.10",
    "@trpc/client": "^10.12.0",
    "@trpc/react-query": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "client-only": "^0.0.1",
    "copy": "^0.3.2",
    "date-fns": "^2.29.3",
    "decimal.js": "^10.4.3",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "encoding": "^0.1.13",
    "env-var": "^7.3.0",
    "form-data": "^4.0.0",
    "js-cookie": "^3.0.1",
    "js-file-downloader": "^1.1.24",
    "nanoid": "^4.0.1",
    "next": "^13.2.1",
    "next-auth": "^4.19.2",
    "node-html-parser": "^6.1.5",
    "nodemailer": "^6.9.1",
    "prisma": "^4.10.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-intersection-observer": "^9.4.3",
    "react-loader-spinner": "^5.3.4",
    "react-textarea-autosize": "^8.4.0",
    "react-use-css-custom-property": "^0.1.1",
    "scroll-into-view-if-needed": "^3.0.6",
    "server-only": "^0.0.1",
    "smooth-scroll-into-view-if-needed": "^1.1.33",
    "styled-components": "^5.3.6",
    "superjson": "^1.12.2",
    "uglify-js": "^3.17.4",
    "undici": "^5.20.0",
    "usehooks-ts": "^2.9.1",
    "uuid": "^9.0.0",
    "zod": "^3.20.6",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@mss/test": "*",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "css-loader": "^6.7.3",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "modclean": "^3.0.0-beta.1",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "react-is": "^18.2.0",
    "require-from-string": "^2.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.9.5"
  }
}
