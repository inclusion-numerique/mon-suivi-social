{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "prisma:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": true
    },
    "dev": {
      "dependsOn": ["^prisma:generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^prisma:generate"],
      "outputs": [".next/**"],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "tsc": {
      "cache": false
    }
  },
  "globalEnv": [
    "CI",
    "CHROMATIC_APP_ID",
    "DATABASE_URL",
    "BRANCH",
    "CDK_FORCE_BRANCH",
    "NAMESPACE",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "PORT",
    "APP",
    "PRODUCTION_URL",
    "PRODUCTION_APP",
    "BASE_URL",
    "NEXT_PUBLIC_SIREN_API_KEY",
    "NEXT_PUBLIC_CONTACT_EMAIL",
    "SIREN_ACCESS_TOKEN",
    "SCW_DEFAULT_REGION",
    "SCW_SECRET_KEY",
    "SCW_ACCESS_KEY",
    "SCW_S3_BUCKET_LOCATION",
    "UPLOADS_BUCKET_ID",
    "SCW_S3_HOST",
    "SCW_ORGANIZATION_ID",
    "SCW_PROJECT_ID",
    "MSS_WEB_IMAGE",
    "NEXT_PUBLIC_INCLUSION_CONNECT_ISSUER",
    "NEXT_PUBLIC_INCLUSION_CONNECT_CLIENT_ID",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "INCLUSION_CONNECT_CLIENT_SECRET",
    "GRIST_TABLE_ID",
    "GRIST_DOCUMENT_ID",
    "GRIST_API_KEY",
    "EMAIL_FROM_ADDRESS",
    "EMAIL_FROM_NAME",
    "SMTP_USERNAME",
    "SMTP_PASSWORD",
    "SMTP_SERVER",
    "SMTP_PORT"
  ]
}